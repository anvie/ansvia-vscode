{
    "name": "ansvia-vscode",
    "displayName": "Ansvia VScode Extension",
    "description": "Ansvia VSCode plugin as helper for developers",
    "publisher": "Ansvia",
    "repository": "https://github.com/anvie/ansvia-vscode",
    "license": "SEE LICENSE IN LICENSE",
    "version": "0.3.14",
    "engines": {
        "vscode": "^1.36.0",
        "node": ">=6.0.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onLanguage:dart",
        "onLanguage:rust",
        "onLanguage:sql"
    ],
    "main": "./out/extension.js",
    "contributes": {
        "commands": [
            {
                "command": "extension.server",
                "title": "ANSVIA: Generate Server code"
            },
            {
                "command": "extension.flutter",
                "title": "ANSVIA: Generate Flutter Code"
            },
            {
                "command": "extension.web",
                "title": "ANSVIA: Generate Web code"
            },
            {
                "command": "extension.code_utils",
                "title": "ANSVIA: Utils"
            }
        ],
        "snippets": [
            {
                "language": "rust",
                "path": "./rust_snippets.json"
            },
            {
                "language": "dart",
                "path": "./dart_snippets.json"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "yarn run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "pretest": "yarn run compile",
        "test": "node ./out/test/runTest.js"
    },
    "devDependencies": {
        "@types/glob": "^7.1.1",
        "@types/node": "^10.12.21",
        "@types/vscode": "^1.36.0",
        "glob": "^7.1.4",
        "typescript": "^3.3.1",
        "vscode-test": "^1.0.2"
    },
    "dependencies": {
        "camel-case": "^3.0.0",
        "clipboardy": "^2.1.0",
        "js-yaml": "^3.13.1",
        "pascal-case": "^2.0.1",
        "snake-case": "^2.1.0"
    }
}
